  "scripts": {
    "dev": "npm-run-all -p watch:graphql next:dev",
    "next:dev": "next dev",
    "prebuild": "node pre-build.js && npm run generate-ts-types",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier . --write",
    "generate-ts-types": "graphql-codegen --config ./graphql/graphql.config.ts",
    "watch:graphql": "chokidar './graphql/**/*.graphql' -c 'npm run generate-ts-types' --debounce 500",
    "create-feature": "node create-feature.mjs",
    "generate-migrations": "node generate-migrations.mjs",
    "check-types": "tsc --noEmit"
